
<div class="container">
			
	<h3>Cтраницы</h3>

		<h4><a href="/sklad/events">Акции</a></h4>
			<h4><a href="/sklad/accessorize">Аксессуары</a></h4>
				<h4><a href="/sklad/autovokzal">Одежда</a></h4>
					<h4><a href="/sklad/citytransport">Обувь</a></h4>
								<h4><a href="/sklad/taxi">Доставка</a></h4>
			
<div class="col-xs-12 col-sm-9">		<h3>Динамические</h3>
		
		<% _.each(sklads, function(sklad) { %>
		<div class="col-xs-12 col-sm-4">
          <div style="background-color:#ddd;padding:13px; margin-bottom:25px;">
		<tr data-id="<%= sklad.id %>" data-model="sklad">
			<p>
			<h4><a href="/sklad/show/<%= sklad.id %>"><%= sklad.title %></a></h4>
		<a href="/sklad/show/<%= sklad.id %>">
			<img src="<%= sklad.img %>"  width="220">
			</a>
			<div >
			
			<small>ccылка:<input value="sklad/show/<%= sklad.name %>"></small>
				</div>
			<% if (session.authenticated && session.User.admin) { %>
		
			
			<td><a href="/sklad/edit/<%= sklad.id %>" class="btn btn-sm btn-warning">Редагувати</a></td>

			<td><form action="/sklad/destroy/<%= sklad.id %>" method="POST">
				<input type="hidden" name="_method" value="Видалити"/>
				<input type="submit" class="btn btn-sm btn-danger" value="Видалити"/>
				<input type="hidden" name="_csrf" value="<%= _csrf %>" />
			</form></td>
		<%} %>
		<td><a href="/sklad/show/<%= sklad.id %>" class="btn btn-sm btn-primary">Переглянути</a></td>
			</p>
		</tr> 
	
</div>

</div>
		<% }) %>
	

</div>
